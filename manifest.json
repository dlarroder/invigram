{
	"manifest_version": 2,
	"name": "invigram",
	"short_name": "invigram",
	"description": "View instagram stories anonymously",
	"version": "1.0.0",
	"version_name": "1.0.0",
	"icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png",
      "256": "icons/256.png"
	},

	"browser_action": {
		"default_icon": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png"
		},
	"default_title": "Toggle On/Off"
	},
	"permissions": [ "https://*.instagram.com/*", "webRequest", "webRequestBlocking" ],
	"background": {
		"scripts": ["./index.js"]
	}
}